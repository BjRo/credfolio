{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "build/**"]
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "fmt": {
      "outputs": []
    },
    "test": {
      "outputs": ["coverage/**", "reports/**"]
    },
    "typecheck": {
      "outputs": []
    }
  }
}
